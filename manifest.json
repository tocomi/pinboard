{
  "manifest_version": 3,
  "name": "Pinboard",
  "version": "0.0.1",
  "description": "Pin your focusing stuff to the new tab page.",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/icon16.svg",
    "48": "icons/icon48.svg",
    "128": "icons/icon128.svg"
  }
}
